@model ToBeImplemented.Domain.ViewModel.Concepts.AddConceptViewModel
<div class="row">
    <form role="form" method="PUT">
        @Html.HiddenFor(m=>m.AuthorId)
        @Html.AntiForgeryToken()
        <div class="form-group">
            @Html.LabelFor(m=>m.Title)
            @Html.TextBoxFor(m=>m.Title,new {@class="form-control"})
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Description)
            @Html.TextAreaFor(m => m.Description, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Tags)
            @Html.TextBoxFor(m => m.Tags, new { @class = "form-control", placeholder = "semicolon separated" })
        </div>
        <button type="submit" class="btn btn-default pull-right">create</button>
    </form>
</div>